/* components/nineImag/cmp.wxss */
.container{
    width: 700rpx;
}
.imagbox{
    position: relative;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;

    width: 700rpx;
    height: 700rpx;
     
    overflow: hidden;

}




.image{
    width: 31%;
    height: 31%;
}
.imgNum{
    position: absolute;
    right: 90rpx;
    bottom: 90rpx;
    padding: 5rpx 10rpx;
    background-color: rgba(0, 0, 0, .7);
    color: #fff;
    font-size: 24rpx;


}



.text-container{
    padding: 0 30rpx;
    font-weight: 40rpx;
    font-size: 40rpx;
    letter-spacing: 2rpx;
}