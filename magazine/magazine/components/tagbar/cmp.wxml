<scroll-view class="container" scroll-x="{{true}}" scroll-into-view="{{magazineId}}" scroll-with-animation="{{true}}" >
<text 
 wx:for="{{magazineTypeArr}}"
 class="type {{index === magazineIndex ? 'active' : ''}}"
 wx:key="{{index}}"
 bind:tap="onTap"
 data-index="{{index}}"
 id="{{'magazine' + index}}"
 >{{item}}</text>
</scroll-view>
